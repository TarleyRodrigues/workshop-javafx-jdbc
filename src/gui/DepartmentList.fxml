<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="369.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.DepartmentListController">
   <children>
      <Label text="Registro de departmento">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <VBox.margin>
            <Insets left="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button fx:id="btNew" mnemonicParsing="false" onAction="#onBtNewAction" text="Novo" textFill="#006804">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <effect>
                  <Reflection />
               </effect></Button>
        </items>
      </ToolBar>
      <TableView fx:id="tableViewDepartment" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tableColumnName" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="tableColumnEDIT" minWidth="50.0" prefWidth="55.0" />
            <TableColumn fx:id="tableColumnREMOVE" minWidth="50.0" prefWidth="55.0" />
        </columns>
      </TableView>
   </children>
</VBox>
